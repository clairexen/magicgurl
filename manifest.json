{
	"manifest_version": 2,
	"name": "Magicgurl",
	"version": "0.0.1",
	"description": "Replaces the t-slur (and similar words) with 'magicgurl' on porn sites",
	"icons": {
		"128": "magicgurl128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.xvideos.com/*"
			],
			"js": ["magicgurl.js"],
			"run_at":"document_end"
		}
	]
}
